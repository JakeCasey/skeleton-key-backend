{
  "name": "skeleton-key-backend",
  "version": "1.0.0",
  "description": "the backend of skeleton key",
  "scripts": {
    "start": "nodemon -e js,graphql -x node src/index.js",
    "dev": "nodemon -e js,graphql -x node src/index.js",
    "deploy": "prisma deploy --env-file variables.env",
    "logs": "prisma cluster logs --env-file variables.env",
    "force-deploy": "prisma deploy --force --env-file variables.env",
    "get-token": "prisma token --env-file variables.env"
  },
  "engines": {
    "node": "14.x"
  },
  "dependencies": {
    "@mailchimp/mailchimp_marketing": "^3.0.66",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "2.4.3",
    "body-parser": "^1.19.0",
    "commander": "^2.19.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "dotenv": "6.0.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "figlet": "^1.2.1",
    "graphql": "^0.13.2",
    "graphql-cli": "^2.16.7",
    "graphql-yoga": "1.16.2",
    "inquirer": "^6.2.2",
    "jsonwebtoken": "8.3.0",
    "knox": "^0.9.2",
    "mjml": "^4.8.1",
    "needle": "^2.5.0",
    "nodemailer": "^4.6.8",
    "nodemon": "1.19.2",
    "prisma-binding": "^2.3.16",
    "stripe": "^8.117.0",
    "telegraf": "^4.4.2"
  },
  "license": "MIT",
  "repository": {},
  "babel": {
    "presets": [
      "env"
    ]
  },
